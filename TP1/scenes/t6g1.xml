<lsf>
    <initials>
        <reference length="2"/>
        <!-- axis length; "0" means no axis displayed -->
        <root id="root"/>
        <!-- identifier of root node of the scene graph; this node -->
        <!-- must be defined in one of the following NODE declarations -->
    </initials>

    <views default="defaultCamera">
        <!-- at least one of the following type of cameras must be declared -->
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="30" y="15" z="30"/>
            <to x="0" y="-2" z="0"/>
        </perspective>
        <ortho id="demoOrtho" near="0.2" far="100" left="-0.2" right="0.2" top="0.2" bottom="-0.2">
            <from x="5" y="0" z="10"/>
            <to x="5" y="0" z="0"/>
            <up x="0" y="1" z="0"/>    <!-- optional, default 0,1,0 -->
        </ortho>
    </views>

    <illumination>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />           <!-- global ambient -->
        <background r="0.1" g="0.7" b="1.0" a="1.0" />        <!-- background color -->
    </illumination>

    <lights>
        <!-- NOTE: this block "light" must be repeated as necessary with different "id".
        At least one light should be present, and a maximum of eight lights is accepted. -->

        <light id="sun">
            <enable value="1"/>
            <position x="10.0" y="15.0" z="9.5" w="1"/>       <!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0"/>
            <diffuse r="1.0" g="0.96" b="0.89" a="1.0"/>
            <specular r="1.0" g="0.96" b="0.89" a="1.0"/>
        </light>
    </lights>

